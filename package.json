    "gulp-jshint": "^2.0.4",
    "gulp-less": "^3.3.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.1.2",
    "gulp-uglify": "~1.1.0",
    "gulp-usemin": "^0.3.28",
    "gulp-watch": "^4.3.11",
    "jshint-stylish": "^2.2.1",
    "run-sequence": "~1.0.2",
