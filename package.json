{
  "name": "js",
  "version": "2.3.0",
  "description": "Japan Retail Summary & Japan Institutional Summary",
  "scripts": {
    "setup": "bower install && npm install",
    "build": "gulp dist",
    "serve": "gulp serve",
    "dev": "gulp dev",
    "test": "gulp test && karma start test\\karma.conf.js",
    "eslint": "eslint \"app/scripts/**/*.js\" -f jslint-xml -o \"test/reports/jslint.xml\" || true",
    "eslint-html": "eslint \"app/scripts/**/*.js\" -f html -o \"test/reports/jslint/index.html\" || true"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "jasmine-core": "^2.7.0",
    "jshint": "^2.9.5",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.4"
  },
  "app": {
    "src": "app",
    "dist": "dist",
    "port": 9000
  },
  "browserslist": [
    "Android 2.3",
    "Android >= 4",
    "Chrome >= 20",
    "Firefox >= 24",
    "Explorer >= 8",
    "iOS >= 6",
    "Opera >= 12",
    "Safari >= 6"
  ],
  "dependencies": {
    "assemble": "^0.24.3",
    "autoprefixer": "^7.1.1",
    "gulp-angular-templatecache": "^2.0.0",
    "gulp-batch-replace": "0.0.0",
